generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid()) @map("_id")  // MongoDBâ€™s _id as primary key
  nip         String @unique
  password    String
  is_active   String
  
}

