generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid()) @map("_id")  // MongoDBâ€™s _id as primary key
  nip         String @unique
  nama        String
  pangkat     String
  jabatan     String
  organisasi  String
  password    String
}